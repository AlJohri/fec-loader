#!/usr/bin/env node
// jshint esnext:true

const models = require('@publici/fec-model')({
    driver: 'postgres',
    name: process.env.PGDATABASE,
    user: process.env.PGUSER,
    pass: process.env.PGPASSWORD,
    host: process.env.PGHOST || 'localhost',
    port: process.env.PGPORT || 5432
});

models.sync(() => {});
